  <DataSets>
    <DataSet Name="ServicesProvided">
      <Query>
        <DataSourceName>PM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@From">
            <Value>=Parameters!From.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@To">
            <Value>=Parameters!To.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT e.Name as ServiceName
	  ,k.FullName as PolicyHolder
	  ,d.FullName as PatientName
	  ,a.ItemDate
	  ,a.[Units] * a.[Price] as Amount
	  --,Sum( a.[Units] * a.[Price]) + Sum(a.[Units] * a.[Tax]) as Amount
	  ,f.Code as InsuranceProvider
	  ,h.PolicyNumber
  
  FROM [dbo].[InvoiceItems] a 
       inner join [dbo].[Invoices] b on (a.InvoiceRowId = b.RowId)
       inner join Appointments c on (a.AppointmentRowId = c.RowId)
	   inner join PatientsView d on (c.PatientRowId = d.RowId)
       inner join MedicalServicesOrSupplies e on (a.ServcieOrSupplyRowId = e.RowId)
	   left outer join InsuranceCoverageHolders g on (c.PatientRowId = g.PolicyHolderRowId)
	   left outer join InsuranceCoverages h on (g.InsuranceCoverageRowId = h.RowId)
	   left outer join InsuranceProviders f on (c.InsuranceProviderRowId = f.RowId )
	   left outer join 
	    (
		  Select t1.InsuranceCoverageRowId, t2.FullName  From InsuranceCoverageHolders t1 inner join PatientsView t2 on (t1.PolicyHolderRowId = t2.RowId and t1.PolicyHolderType = 'Policy Owner')
		) k on (k.InsuranceCoverageRowId = h.RowId)
	   
--Group By 	   d.FullName
--		,e.Name
--		,b.InvoiceDate
--		,f.Code
--		,h.PolicyNumber
--		,k.FullName

WHERE a.ItemDate BETWEEN (@From) and (@To)
Order By d.FullName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ServiceName">
          <DataField>ServiceName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PolicyHolder">
          <DataField>PolicyHolder</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PatientName">
          <DataField>PatientName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ItemDate">
          <DataField>ItemDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Amount">
          <DataField>Amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="InsuranceProvider">
          <DataField>InsuranceProvider</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PolicyNumber">
          <DataField>PolicyNumber</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="OutstandingInvoices">
      <Query>
        <DataSourceName>PM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@From">
            <Value>=Parameters!From.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@To">
            <Value>=Parameters!To.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select a.InvoiceDate,
       c.FullName,
	   mm.ServiceProvider,
	   mm.Amount as InvoiceAmount,
	   Sum(r.Amount) as AllocatedPayments

From Invoices a inner join (
						Select a.InvoiceRowId,
							   s.LastName + ', ' + s.FirstName as ServiceProvider,
							   Sum(a.Units*(a.Price + a.Tax)) as Amount
							   --m.Name as ServiceNAme	   
						 From InvoiceItems a left outer join Appointments b on (a.AppointmentRowId = b.RowId)
											 left outer join ServiceProviders s on (b.ServiceProviderRowId = s.RowId)
											 --left outer join MedicalServicesOrSupplies m on (b.MedicalServicesOrSupplyRowId = m.RowId)
											 Group by a.InvoiceRowId, s.LastName + ', ' + s.FirstName--, m.Name 
                          ) mm on a.RowId = mm.InvoiceRowId
     left outer join  PatientsView c on (a.PatientRowId = c.RowId)
	 left outer join Payments p on (p.PatientRowId = c.RowId)
	 left outer join InvoicePayments r on (p.RowId = r.PaymentRowId)
Where a.InvoiceDate BETWEEN @From and @To
Group By a.InvoiceDate, c.FullName, mm.ServiceProvider, mm.Amount
Order By c.Fullname

</CommandText>
      </Query>
      <Fields>
        <Field Name="InvoiceDate">
          <DataField>InvoiceDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="FullName">
          <DataField>FullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ServiceProvider">
          <DataField>ServiceProvider</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="InvoiceAmount">
          <DataField>InvoiceAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="AllocatedPayments">
          <DataField>AllocatedPayments</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
