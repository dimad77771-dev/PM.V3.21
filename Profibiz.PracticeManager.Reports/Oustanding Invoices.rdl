    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>PM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@From">
            <Value>=Parameters!From.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@To">
            <Value>=Parameters!To.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select a.InvoiceDate,
       c.FullName,
	   mm.ServiceProvider,
	   mm.Amount as InvoiceAmount,
	   Sum(r.Amount) as AllocatedPayments

From Invoices a inner join (
						Select a.InvoiceRowId,
							   s.LastName + ', ' + s.FirstName as ServiceProvider,
							   Sum(a.Units*(a.Price + a.Tax)) as Amount
							   --m.Name as ServiceNAme	   
						 From InvoiceItems a left outer join Appointments b on (a.AppointmentRowId = b.RowId)
											 left outer join ServiceProviders s on (b.ServiceProviderRowId = s.RowId)
											 --left outer join MedicalServicesOrSupplies m on (b.MedicalServicesOrSupplyRowId = m.RowId)
											 Group by a.InvoiceRowId, s.LastName + ', ' + s.FirstName--, m.Name 
                          ) mm on a.RowId = mm.InvoiceRowId
     left outer join  PatientsView c on (a.PatientRowId = c.RowId)
	 left outer join Payments p on (p.PatientRowId = c.RowId)
	 left outer join InvoicePayments r on (p.RowId = r.PaymentRowId)
Group By a.InvoiceDate, c.FullName, mm.ServiceProvider, mm.Amount
Order By c.Fullname</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
